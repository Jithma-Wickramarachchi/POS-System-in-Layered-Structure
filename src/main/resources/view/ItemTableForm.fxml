<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="itemTablePane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ItemTableFormController">
   <children>
      <TableView fx:id="itemsTable" layoutX="14.0" layoutY="84.0" prefHeight="502.0" prefWidth="775.0">
        <columns>
          <TableColumn fx:id="colItemCode" prefWidth="86.0" text="Item Code" />
          <TableColumn fx:id="colDesc" prefWidth="198.0" text="Description" />
            <TableColumn fx:id="colUnitPrice" minWidth="0.0" prefWidth="147.0" text="Unit Price" />
            <TableColumn fx:id="colQty" prefWidth="146.0" text="QTY" />
            <TableColumn fx:id="colEdit" editable="false" prefWidth="96.0" sortable="false" text="Edit" />
            <TableColumn fx:id="colDelete" editable="false" prefWidth="101.0" sortable="false" text="Delete" />
        </columns>
      </TableView>
      <Label alignment="CENTER" layoutX="297.0" layoutY="30.0" prefHeight="41.0" prefWidth="206.0" text="Items Table">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="28.0" layoutY="30.0" mnemonicParsing="false" onAction="#backBtnOnAction" text="Back" />
   </children>
</AnchorPane>
